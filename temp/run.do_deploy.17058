		initramfs_symlink_name=zImage-initramfs-imx6qsabresd
		install -m 0644 arch/arm/boot/zImage.initramfs /home/ati-openrex/fsl-community-bsp-own/build/tmp/work/imx6qsabresd-poky-linux-gnueabi/linux-fslc-imx/3.14-1.1.x+gitAUTOINC+327d5c9063-r0/deploy-linux-fslc-imx/${initramfs_base_name}.bin
		cd /home/ati-openrex/fsl-community-bsp-own/build/tmp/work/imx6qsabresd-poky-linux-gnueabi/linux-fslc-imx/3.14-1.1.x+gitAUTOINC+327d5c9063-r0/deploy-linux-fslc-imx
