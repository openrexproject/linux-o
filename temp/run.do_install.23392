		for DTB in imx6q-sabresd.dtb imx6q-sabresd-ldo.dtb imx6q-sabresd-hdcp.dtb imx6q-sabresd-enetirq.dtb imx6q-sabresd-btwifi.dtb; do
			if echo ${DTB} | grep -q '/dts/'; then
